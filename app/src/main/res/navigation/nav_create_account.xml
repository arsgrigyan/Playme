<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    app:startDestination="@id/createAccountFirstScreenFragment">

    <fragment
        android:id="@+id/createAccountFirstScreenFragment"
        android:name="com.southernsunrise.playme.fragments.authenticationScreens.registerScreens.CreateAccountFirstScreenFragment"
        android:label="fragment_create_account_first_screen"
        tools:layout="@layout/fragment_create_account_first_screen">
        <action
            android:id="@+id/action_createAccountFirstScreenFragment_to_createAccountSecondScreenFragment"
            app:destination="@id/createAccountSecondScreenFragment"
            app:enterAnim="@anim/anim_enter_from_right"
            app:exitAnim="@anim/anim_exit_to_left"
            app:popEnterAnim="@anim/anim_enter_from_left"
            app:popExitAnim="@anim/anim_exit_to_right" />
    </fragment>
    <fragment
        android:id="@+id/createAccountSecondScreenFragment"
        android:name="com.southernsunrise.playme.fragments.authenticationScreens.registerScreens.CreateAccountSecondScreenFragment"
        android:label="fragment_create_account_second_screen"
        tools:layout="@layout/fragment_create_account_second_screen">
        <action
            android:id="@+id/action_createAccountSecondScreenFragment_to_createAccountThirdScreenFragment"
            app:destination="@id/createAccountThirdScreenFragment"
            app:enterAnim="@anim/anim_enter_from_right"
            app:exitAnim="@anim/anim_exit_to_left"
            app:popEnterAnim="@anim/anim_enter_from_left"
            app:popExitAnim="@anim/anim_exit_to_right"/>
    </fragment>
    <fragment
        android:id="@+id/createAccountThirdScreenFragment"
        android:name="com.southernsunrise.playme.fragments.authenticationScreens.registerScreens.CreateAccountThirdScreenFragment"
        android:label="fragment_create_account_third_screen"
        tools:layout="@layout/fragment_create_account_third_screen">
        <action
            android:id="@+id/action_createAccountThirdScreenFragment_to_createAccountFourthScreenFragment"
            app:destination="@id/createAccountFourthScreenFragment"
            app:enterAnim="@anim/anim_enter_from_right"
            app:exitAnim="@anim/anim_exit_to_left"
            app:popEnterAnim="@anim/anim_enter_from_left"
            app:popExitAnim="@anim/anim_exit_to_right"/>
    </fragment>
    <fragment
        android:id="@+id/createAccountFourthScreenFragment"
        android:name="com.southernsunrise.playme.fragments.authenticationScreens.registerScreens.CreateAccountFourthScreenFragment"
        android:label="fragment_create_account_fourth_screen"
        tools:layout="@layout/fragment_create_account_fourth_screen">
        <action
            android:id="@+id/action_createAccountFourthScreenFragment_to_createAccountFifthScreenFragment"
            app:destination="@id/createAccountFifthScreenFragment"
            app:enterAnim="@anim/anim_enter_from_right"
            app:exitAnim="@anim/anim_exit_to_left"
            app:popEnterAnim="@anim/anim_enter_from_left"
            app:popExitAnim="@anim/anim_exit_to_right"/>
    </fragment>
    <fragment
        android:id="@+id/createAccountFifthScreenFragment"
        android:name="com.southernsunrise.playme.fragments.authenticationScreens.registerScreens.CreateAccountFifthScreenFragment"
        android:label="fragment_create_account_fifth_screen"
        tools:layout="@layout/fragment_create_account_fifth_screen" />
</navigation>